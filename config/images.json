  "optimization": {
    "quality": 85,
    "formats": ["webp", "avif", "jpg", "png"],
    "breakpoints": [640, 1024, 1920, 3840],
    "lazyOffset": 100,
    "preload": ["hero", "profile", "header-logo"],
    "cache": {
      "enabled": true,
      "maxSize": 50,
      "ttl": 3600000
    }
  },
  "effects": {
    "glitch": {
      "intensity": 0.8,
      "frequency": 3000,
      "duration": 200
    },
    "scanlines": {
      "opacity": 0.03,
      "speed": 8,
      "spacing": 4
    },
    "vhs": {
      "distortion": 0.05,
      "noise": 0.02,
      "tracking": 0.01
    },
    "pixel": {
      "scale": 4,
      "smoothing": false
    },
    "crt": {
      "curvature": 0.02,
      "vignette": 0.3,
      "brightness": 1.05,
      "contrast": 1.1
    },
    "matrix": {
      "color": "#00ff00",
      "speed": 50,
      "density": 0.95
    },
    "parallax": {
      "speed": 0.5,
      "depth": 3
    },
    "noise": {
      "opacity": 0.02,
      "blend": "overlay"
    }
  }
}
